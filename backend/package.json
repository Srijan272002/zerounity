{
  "name": "backend",
  "version": "1.0.0",
  "description": "Backend API server",
  "main": "dist/index.js",
  "scripts": {
    "start": "node -r tsconfig-paths/register dist/index.js",
    "dev": "ts-node -r tsconfig-paths/register src/index.ts",
    "build": "tsc --build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "function:dev": "functions-framework --target=generateText --source=src/functions/generate-text.ts",
    "function:build": "tsc --project src/functions",
    "function:deploy": "gcloud functions deploy generateText --gen2 --runtime=nodejs18 --region=us-central1 --source=. --entry-point=generateText --trigger-http"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@google-cloud/functions-framework": "^3.3.0",
    "@google-cloud/storage": "^7.7.0",
    "@google/generative-ai": "^0.2.1",
    "@supabase/auth-helpers-nextjs": "^0.10.0",
    "@supabase/supabase-js": "^2.39.0",
    "@trpc/server": "^10.45.0",
    "@zero/shared": "file:../shared",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^4.18.2",
    "express-fileupload": "^1.5.1",
    "express-rate-limit": "^7.1.5",
    "ffmpeg-static": "^5.2.0",
    "ioredis": "^5.6.1",
    "sharp": "^0.33.2",
    "socket.io": "^4.8.1",
    "superjson": "^2.2.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.22",
    "@types/express-fileupload": "^1.5.1",
    "@types/express-rate-limit": "^5.1.3",
    "@types/ioredis": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.17.57",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/sharp": "^0.31.1",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.33.1",
    "@typescript-eslint/parser": "^8.33.1",
    "jest": "^29.7.0",
    "next": "^14.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.2"
  }
}
